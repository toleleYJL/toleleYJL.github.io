    <script type="text/javascript">
      const splash = document.getElementById('splash');
      const browserError = document.getElementById('browser-error');
      const localTheme = localStorage.getItem('theme_mode');
      const openSuperLoading = localStorage.getItem('super-loading');
      splash.style.display = 'flex';
      browserError.remove();

      if (openSuperLoading) {
        const normalLoading = document.getElementById('normal-loading');
        const superLoading = document.getElementsByClassName('super-loading')[0];
        normalLoading.style.display = 'none';
        superLoading.style.display = 'block';
      }
      if (!localTheme || localTheme === 'system') {
        splash.classList.add('splash-system');
      } else if (localTheme) {
        splash.classList.add(localTheme === 'dark' ? 'splash-dark' : 'splash-light');
      }
    </script>

    
    
    
    
